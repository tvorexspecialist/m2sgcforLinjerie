{
  "version": "1.0.4",
  "id": "@shopgate/magento-cart",
  "directories": {
    "steps": "./extension/",
    "pipelines": "./pipelines/"
  },
  "steps": [],
  "configuration": {
    "enableCartCoupons" : {
      "type" : "static",
      "destination" : "backend",
      "params" : {
        "value" : true
      }
    },
    "magentoUrl" : {
      "type": "bigApi",
      "destination" : "backend",
      "default": null,
      "params": {
        "method":  "GET",
        "service": "config",
        "path": "/v1/shop/%(shopId)s/checkout_web_checkout_shopify?parsed=true",
        "key": "value.backend.magentoUrl"
      }
    }
  }
}
