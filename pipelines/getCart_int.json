          {"key": "magentoCart", "id": "3"}
        ],
        "output": [
          {"key": "productIds", "id": "4"},
          {"key": "limit", "id": "5"},
          {"key": "offset", "id": "6"},
          {"key": "characteristics", "id": "8"}
        ]
      }, {
        "type": "pipeline",
        "id": "getProducts_v1",
        "input": [
          {"key": "sgxsMeta", "id": "750"},
          {"key": "productIds", "id": "4"},
          {"key": "limit", "id": "5"},
          {"key": "offset", "id": "6"},
          {"key": "characteristics", "id": "8"}
        ],
        "output": [
          {"key": "products", "id": "7"}
        ]
      }, {
        "type": "extension",
        "id": "@shopgate/magento-cart",
        "path": "@shopgate/magento-cart/cart/transformToShopgateCart.js",
        "input": [
          {"key": "magentoCart", "id": "3"},
          {"key": "shopgateProducts", "id": "7"}
        ],
        "output": [
          {"key": "cart", "id": "100"}
        ]
      }
    ]
  }
}