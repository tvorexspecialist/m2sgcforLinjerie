    "output": [
      {"key": "cart", "id": "100", "optional": true}
    ],
    "steps": [
      {
        "type": "extension",
        "id": "@shopgate/magento-cart",
        "path": "@shopgate/magento-cart/token/getMagentoToken.js",
        "input": [],
        "output": [
          {"key": "tokens", "id": "10"}
        ]
      }, {
        "type": "extension",
        "id": "@shopgate/magento-cart",
        "path": "@shopgate/magento-cart/cart/getCartFromMagento.js",
        "input": [
          {"key": "tokens", "id": "10"},
          {"key": "cartId", "id": "1"}
        ],
        "output": [
          {"key": "magentoCart", "id":"3"}
        ]
      }, {
        "type": "extension",
        "id": "@shopgate/magento-cart",
        "path": "@shopgate/magento-cart/cart/getShopgateProductIds.js",
